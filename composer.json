{
    "name": "bataboom/h-captcha",
    "description": "hCAPTCHA For Laravel.",
    "keywords": [
        "h-captcha",
        "hcaptcha",
        "captcha",
        "laravel",
        "laravel4",
        "laravel5",
        "laravel6",
        "laravel10"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "bataboom",
            "email": "contact@bataboom.dev"
        }
    ],
    "require": {
        "php": ">=5.5.5",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^6.2|^7.0",
        "filament/forms": "^2.16|^3.0-stable|^3.2.51"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8|^9.5.10|^10.5"
    },
    "autoload": {
        "psr-4": {
            "BataBoom\\Captcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BataBoom\\Captcha\\HCaptchaServiceProvider"
            ],
            "aliases": {
                "HCaptcha": "BataBoom\\Captcha\\Facades\\HCaptcha"
            }
        }
    }
}
